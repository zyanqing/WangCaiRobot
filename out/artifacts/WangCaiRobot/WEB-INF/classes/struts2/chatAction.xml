<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>

    <package name="struts_chatAction" namespace="/" extends="struts-default">

        <action name="manageCaht_*" class="action.ChatAction" method="{1}">

            <result name="deletChat">/jsp/list.jsp</result>
            <result name="cleanChat">/jsp/list.jsp</result>

            <allowed-methods>talk,deletChat,cleanChat</allowed-methods>

        </action>


    </package>

</struts>